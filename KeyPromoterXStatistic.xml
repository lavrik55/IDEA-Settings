<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Add to VCS">
        <Item>
          <IdeaActionID>ChangesView.AddUnversioned.From.Dialog</IdeaActionID>
          <ShortCut>'Ctrl+Alt+A'</ShortCut>
          <Description>Add to VCS</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Ctrl+K'</ShortCut>
          <Description>Commit...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Generate...">
        <Item>
          <IdeaActionID>Generate</IdeaActionID>
          <ShortCut>'Alt+Insert'</ShortCut>
          <Description>Generate...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Push...">
        <Item>
          <IdeaActionID>Vcs.Push</IdeaActionID>
          <ShortCut>'Ctrl+Shift+K'</ShortCut>
          <Description>Push...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Main'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'Main'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'MainEx6.main()'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F10'</ShortCut>
          <Description>Run 'MainEx6.main()'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Over">
        <Item>
          <IdeaActionID>StepOver</IdeaActionID>
          <ShortCut>'F8'</ShortCut>
          <Description>Step Over</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Java Class">
        <Item>
          <IdeaActionID>NewClass</IdeaActionID>
          <Description>Java Class</Description>
        </Item>
      </Statistic>
    </option>
  </component>
</application>